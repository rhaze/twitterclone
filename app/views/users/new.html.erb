<h1>Register</h1>

<%= simple_form_for @user do |f| %>
	<%= f.input :first_name %>
	<%= f.input :last_name %>
	<%= f.input :email %>
	<%= f.input :username %>
	<%= f.input :password %>
	<%= f.input :password_confirmation %>
	<br>
	<%= f.submit "Register", class: 'btn btn-primary'%>
	<%= link_to "Cancel", root_url, class: 'btn btn-primary' %>
	<br>
<% end %>